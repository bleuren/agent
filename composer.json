{
    "name": "bleuren/agent",
    "description": "Enhanced user agent parser for Laravel",
    "keywords": [
        "laravel",
        "user agent",
        "parser",
        "browser detect",
        "os detect",
        "device detect",
        "mobile detect",
        "robot detect",
        "crawler detect"
    ],
    "homepage": "https://github.com/bleuren/agent",
    "license": "MIT",
    "authors": [
        {
            "name": "Bleuren",
            "email": "bleuren421@gmail.com",
            "role": "Developer"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.3",
        "mobiledetect/mobiledetectlib": "^4.8",
        "jaybizzle/crawler-detect": "^1.2.118"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Bleuren\\Agent\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bleuren\\Agent\\AgentServiceProvider"
            ],
            "aliases": {
                "Agent": "Bleuren\\Agent\\Facades\\Agent"
            }
        }
    }
}